{
	"$schema": "https://turborepo.com/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".next/**", "!.next/cache/**"]
		},
		"format": {
			"dependsOn": ["^format"],
			"outputs": []
		},
		"lint": {
			"dependsOn": ["^format", "^lint"]
		},
		"typecheck": {
			"dependsOn": ["^typecheck"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"start": {
			"dependsOn": ["^start"]
		}
	}
}
